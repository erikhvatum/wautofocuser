cdef extern from "cpp_lib/_Highpass.h":
    cdef cppclass _Highpass:
        _Highpass()
        int get_foo()

cdef class Highpass:
    cdef _Highpass *thisptr

    def __cinit__(self):
        self.thisptr = new _Highpass()

    def __dealloc__(self):
        del self.thisptr

    def get_foo(self):
        return self.thisptr.get_foo()
